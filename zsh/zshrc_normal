source $HOME/.profile

export ZSH=$HOME/.oh-my-zsh

ZSH_THEME="avit"

HYPHEN_INSENSITIVE="true"

export UPDATE_ZSH_DAYS=10

COMPLETION_WAITING_DOTS="true"

HIST_STAMPS="yyyy-mm-dd"

plugins=(archlinux autojump colored-man docker docker-compose gem git git-extras go golang man npm pip tmux tmuxniator vim-interaction)

# user configuration

source $ZSH/oh-my-zsh.sh
eval `dircolors ~/.dircolors.256dark`

# gruvbox
source ~/.vim/plugged/gruvbox/gruvbox_256palette.sh

# alias
alias q="exit"
alias ..="cd .."
alias caja="caja ."
alias v="vim"
alias gittoken="cat ~/Documents/backup_codes_tokens/github_token"
alias rm="trash-put"
